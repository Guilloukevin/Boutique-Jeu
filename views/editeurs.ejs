<%- include('./partials/head')-%>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Liste des editeurs.</h1>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Nom</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
            <% editeurs.forEach(function(editeur) {%>
                <tr>
                    <th scope="row"><%=editeur.editeurId%></th>
                    <td><%=editeur.nomEditeur%></td>
                    <td>
                        <button type="button" class="btn btn-primary">Editer</button>
                        <button type="button" class="btn btn-danger">Supprimer</button>
                    </td>
                </tr>
            <%});%>
        </tbody>
      </table>
    </div>
  </div>
</div>
  <%- include('./partials/footer')-%>