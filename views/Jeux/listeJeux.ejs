<%- include('../partials/head')-%>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Liste des Jeux.</h1>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Nom</th>
            <th scope="col">Prix</th>
            <th scope="col">Editeur</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
            <% consoles.forEach(function(console) {%>
                <tr>
                    <th scope="row"><%=console.JeuId%></th>
                    <td><%=console.Titre%></td>
                    <td><%=console.Prix%></td>
                    <td><%=console.editeurId%></td>
                    <td>
                        <a href="/jeux/modifier-jeux"> <input type="button" value="Modifier un jeu" class="btn btn-primary"></a>
                        <a class="btn btn-danger delete" onclick="return alert('Are You sure?')" href="/jeux/liste-jeux/<%=console.JeuId%>">Supprimer</a> 
                        <a href="/jeux/ajouter-jeux"> <input type="button" value="Ajouter un jeu" class="btn btn-danger"></a>
                    </td>
            <%});%>
        </tbody>
      </table>
    </div>
  </div>
</div>
  <%- include('../partials/footer')-%>	